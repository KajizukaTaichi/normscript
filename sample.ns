function fizzbuzz (n) {
    let fizz? = (n % 3 == 0);
    let buzz? = (n % 5 == 0);
    let msg = "";

    if fizz? {
        let msg = "Fizz";
    };
    if buzz? {
        let msg = msg + "Buzz";
    };
    if (msg == "") {
        let msg = n;
    };
    msg
};

print join(map(range(1, 100 + 1, 1), fizzbuzz), new-line)
