function prime (n) {
    let nums = range(0, n + 1, 1);
    set nums[0] = false;
    set nums[1] = false;

    for i in range(2, n ^ 0.5 + 1, 1) {
        if (nums[i]) {
            for j in range(i * i, n + 1, i) {
                set nums[j] = false;
            }
        }
    };

    filter(nums, function(n){ !(n == false) })
}

print "Prime Apps" + new-line;
let n = cast(input("> "), "number");
print join(prime(n), " ") + new-line;
